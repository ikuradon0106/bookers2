# indexページの左側

<h1>User info</h1>

<%= form_with model: @book do |f| %>
  <h4>image</h4>
  <%= f.file_field :image, accept: "image/*" %>
  <h4>name</h4>
  <%= f.text_filed :name %>
  <h4>introduction</h4>
  <%= f.text_area :introduction %>
  <%= f.submit 'edit' %>
<% end %>



<h1>New book</h1>

<%= form_with model: @book do |f| %>
  <h4>Title</h4>
  <%= f.text_filed :name %>
  <h4>Opinion</h4>
  <%= f.text_filed :name %>
  <%= f.submit 'Create Book' %>
<% end %>

